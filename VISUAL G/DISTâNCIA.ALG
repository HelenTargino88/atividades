Algoritmo "distância"

Var
m, cm, km, hm, dam, dm, mm: real


Inicio
escreva("Metros: ")
leia(m)
cm <- m * 100
escreval(cm , " cm")
km <- m * 1000
escreval(km , " km")
hm <- m * 100
escreval(hm , " hm")
dam <- m * 10
escreval(dam , " dam")
dm <- m * 10
escreval(dm , " dm")
mm <- m * 100
escreval(mm , " mm")

Fimalgoritmo